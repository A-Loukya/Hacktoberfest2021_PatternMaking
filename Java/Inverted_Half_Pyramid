// Code for Inverted Half Pyramid pattern
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = n;
        for(int i = 0; i < n; i++){
            for(int j = 0; j < m; j++){
                System.out.print("*\t");
            }
            m--;
            System.out.println();
        }
    }
}
