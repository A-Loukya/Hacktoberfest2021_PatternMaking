import java.util.*;
import java.lang.*;
import java.io.*;


class HalfNumberPyramid
{
	public static void main (String[] args) throws java.lang.Exception
	{
		for(int i = 1;i<=5;i++)
		{
		    int c = 1;
		    for(int j = 1;j<=i;j++)
		    {
		        System.out.print(c+" ");
		        c++;
		    }
		    System.out.println();
		}
	}
}
